{
  "name": "fairfood_collector",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "start": "npx react-native start",
    "run-dev": "ENVFILE=.env.development npx react-native run-android",
    "run-prod": "ENVFILE=.env.production npx react-native run-android",
    "debug": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "clean": "cd android && ./gradlew clean && cd ../",
    "apk-dev": "cd android && ENVFILE=.env.development ./gradlew assembleRelease && cd ../",
    "apk-prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd ../",
    "aab-dev": "cd android && ENVFILE=.env.development ./gradlew bundleRelease && cd ../",
    "aab-prod": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ../",
    "test": "jest",
    "lint": "eslint .",
    "codepush_app": "ENVFILE=.env.production appcenter codepush release-react -a shyno-fairfood.org/fairfood_collector -d Production",
    "prepare": "husky install",
    "precommit": "yarn lint"
  },
